	"dependencies": {
		"@nextui-org/react": "^2.4.1",
		"classnames": "^2.5.1",
		"framer-motion": "^11.2.10",
		"next": "^14.2.3",
		"next-themes": "^0.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"reset-css": "^5.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/node": "^20.13.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"eslint": "^8.57.0",
		"eslint-config-next": "^14.2.3",
