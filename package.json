		"sass": "^1.77.4",
		"stylelint": "^16.6.1",
		"stylelint-config-css-modules": "^4.4.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"stylelint-scss": "^6.3.0",
		"svgo": "^3.3.2",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"typescript-plugin-css-modules": "^5.1.0"
	}
